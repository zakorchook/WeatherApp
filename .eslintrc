{
    "parser": "babel-eslint",
    "extends": "airbnb/base",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "_": false,
        "$": false,
        "moment": false,
        "google": false,
    },
    "rules": {
        "semi": 0,
        "no-shadow": 0,
        "func-names": 0,
        "indent": ["error", 4, {"SwitchCase": 1}],
        "object-curly-spacing": 0,
        "max-len": ["error", 100, 4],
        "consistent-return": 0,
        "no-confusing-arrow": "off",
        "new-cap": 2,
        generator-star-spacing: 0
    },
    "ecmaFeatures": {
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "import"
    ]
}
